# Environment Configuration
# Copy this file to .env and configure your FTE connection settings

# FTE WebSocket URL (wss:// for production, ws:// for development)
VITE_FTE_WS_URL=ws://localhost:8080/ws

# FTE REST API URL
VITE_FTE_API_URL=http://localhost:8080/api/v1

# WebSocket subprotocol version
VITE_FTE_WS_SUBPROTOCOL=fte.v1

# Authentication (optional, set if using JWT)
# VITE_FTE_AUTH_TOKEN=your_jwt_token_here

# Connection settings
VITE_FTE_RECONNECT_INTERVAL=3000
VITE_FTE_PING_INTERVAL=30000

# Data update interval (ms)
VITE_FTE_UPDATE_INTERVAL=250

# Debug mode
VITE_DEBUG_MODE=false
